{
    "name": "neilmillard/slim3-skeleton",
    "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "authors": [
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        },
      {
        "name": "Neil Millard",
        "email": "neil@neilmillard.com"
      }
    ],
    "repositories": [
      {
        "type": "artifact",
        "url": "artifact/"
      },{
        "packagist": false
      }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "require": {
      "slim/slim": "^3.0",
      "slim/twig-view": "dev-master",
      "slim/flash": "dev-master",
      "monolog/monolog": "^1.13",
      "gabordemooij/redbean": "dev-master"
    },
    "minimum-stability": "dev"
}
