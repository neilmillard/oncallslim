{
    "name": "neilmillard/slim3-skeleton",
    "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
    "license": "BSD-3-Clause",
    "type": "project",
    "keywords": ["slim-framework", "skeleton"],
    "authors": [
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        },
      {
        "name": "Neil Millard",
        "email": "neil@neilmillard.com"
      }
    ],
    "repositories": [
      {
        "type": "artifact",
        "url": "artifact/"
      },{
        "packagist": false
      }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        }
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "^1.1",
        "slim/flash": "dev-master",
        "monolog/monolog": "^1.13",
        "jeremykendall/password-validator": "3.*",
        "illuminate/database": "5.1.*"
    },
    "minimum-stability": "dev"
}
